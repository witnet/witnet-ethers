{
  "name": "@witnet/ethers",
  "version": "1.1.2",
  "description": "Wit/Oracle Ethers SDK Framework package for EVM-compatible projects",
  "author": "Witnet Foundation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/witnet/witnet-solidity/"
  },
  "private": false,
  "type": "commonjs",
  "bin": {
    "witeth": "src/bin/solidity.js",
    "witnet-ethers": "src/bin/solidity.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/lib/*.d.ts",
      "require": "./dist/src/lib/index.js",
      "default": "./dist/src/lib/index.js"
    },
    "./utils": {
      "types": "./dist/src/lib/*.d.ts",
      "require": "./dist/src/lib/utils.js",
      "default": "./dist/src/lib/utils.js"
    }
  },
  "files": [
    "dist",
    "src/bin",
    "witnet",
    ".env_witnet"
  ],
  "types": "./dist",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "fmt": "eslint --ext .js,.ts,.cjs . --ignore-path .gitignore",
    "fmt!": "eslint --ext .js,.ts,.cjs . --fix --ignore-path .gitignore",
    "prepare": "pnpm run build",
    "postinstall": "node src/bin/postinstall.js",
    "test": "cross-env-shell WITNET_SDK_RADON_ASSETS_PATH='../../witnet/assets' node ./src/bin/solidity.js"
  },
  "keywords": [
    "evm",
    "ethereum",
    "oracle",
    "randomness",
    "solidity",
    "web3",
    "witnet",
    "wit/oracle"
  ],
  "dependencies": {
    "@witnet/sdk": "^1.1.1",
    "axios": "^1.12.2",
    "cbor": "^10.0.11",
    "cross-env": "^10.0.0",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "ethrpc-gateway": "^2.2.9",
    "inquirer": "^8.2.7",
    "json-bigint": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "moment": "^2.30.1",
    "witnet-solidity-bridge": "^2.2.2"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.18.6",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "typescript": "^5.9.2"
  }
}
